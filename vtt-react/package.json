{
  "name": "vtt-react",
  "version": "0.1.3",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^16.6.1",
    "firebase": "^12.0.0",
    "interactjs": "^1.10.27",
    "openai": "^4.97.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-icons": "^5.5.0",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "react-tooltip": "^5.28.0",
    "recharts": "^2.15.1",
    "socket.io-client": "^4.8.1",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "uuid": "^10.0.0",
    "vm-browserify": "^1.1.2",
    "web-vitals": "^4.2.4",
    "zustand": "^4.5.6"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--max-old-space-size=8192 && craco start",
    "start-original": "react-scripts start",
    "start-legacy": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=8192 && craco start",
    "start-port-3001": "set PORT=3001 && set NODE_OPTIONS=--max-old-space-size=8192 && craco start",
    "build": "set NODE_OPTIONS=--max-old-space-size=8192 && craco build",
    "build:netlify": "set GENERATE_SOURCEMAP=false && set CI=true && set NODE_OPTIONS=--max-old-space-size=8192 && set REACT_APP_BUILD_MODE=production && rmdir /s /q build 2>nul && npm run build",
    "build-original": "react-scripts build",
    "serve:production": "npx serve -s build -l 3000",
    "netlify:local": "npm run build:netlify && npm run serve:production",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "@babel/preset-react": "^7.26.3",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "serve": "^14.2.4"
  }
}
